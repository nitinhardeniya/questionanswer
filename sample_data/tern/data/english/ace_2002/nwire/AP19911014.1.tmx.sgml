<TIMEX2 val="1991-10-14" mod="" set="" non_specific="" anchor_val="" anchor_dir="" comment=""><STORY_REF_TIME>October 14, 1991, Monday</STORY_REF_TIME></TIMEX2>, <TIMEX2 val="1991-10-14TMO" mod="" set="" non_specific="" anchor_val="" anchor_dir="" comment="">AM</TIMEX2> cycle 
